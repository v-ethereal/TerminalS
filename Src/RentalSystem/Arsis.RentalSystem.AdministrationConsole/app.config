<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="spring">
      <section name="context" type="Spring.Context.Support.ContextHandler, Spring.Core" />
      <section name="objects" type="Spring.Context.Support.DefaultSectionHandler, Spring.Core" />
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="Arsis.RentalSystem.AdministrationConsole.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="Arsis.RentalSystem.AdministrationConsole.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
  </configSections>

  <spring>
    <context autowire="true">
      <resource uri="~/Bindings.config"/>
    </context>
  </spring>

  <log4net>
    <appender name="GeneralAppender" type="log4net.Appender.RollingFileAppender">
      <appendToFile value="true" />
      <maximumFileSize value="500KB"/>
      <maxSizeRollBackups value="0"/>
      <rollingStyle value="Date"/>
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <file value="Logs/AppLog.log" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%date{yyyy-MM-dd HH:mm:ss}] [%level] [%logger{1}] - %message%newline" />
      </layout>
    </appender>
    <root name="Arsis.RentalSystem.AdministrationConsole">
      <level value="ALL" />
      <appender-ref ref="GeneralAppender" />
    </root>
  </log4net>

  <connectionStrings>
    <add name="RentalSystemConnectionString"
        connectionString="Data Source=localhost\sqlexpress;Initial Catalog=RentalSystem;Integrated Security=True"
        providerName="System.Data.SqlClient" />
	  
    <add name="ContractorProviderConnectionString"
        connectionString="Dsn=Domoded1S"
  providerName="System.Data.Odbc" />
  </connectionStrings>
  <userSettings>
    <Arsis.RentalSystem.AdministrationConsole.Properties.Settings>
      <setting name="LoginName" serializeAs="String">
        <value />
      </setting>
    </Arsis.RentalSystem.AdministrationConsole.Properties.Settings>
  </userSettings>
  <applicationSettings>
    <Arsis.RentalSystem.AdministrationConsole.Properties.Settings>
      <setting name="ParentClientCode" serializeAs="String">
        <value>00000006</value>
      </setting>
      <setting name="TablePrintShowPreview" serializeAs="String">
        <value>false</value>
      </setting>
    </Arsis.RentalSystem.AdministrationConsole.Properties.Settings>
  </applicationSettings>
</configuration>