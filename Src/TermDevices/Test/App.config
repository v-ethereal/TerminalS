<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <remove name="Default" />
        <add name="OwnListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="Terminal.log" />
      </listeners>
    </trace>
  </system.diagnostics>

</configuration>
